{
  "name": "rps",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "npm run build:clean && npm run build:server",
    "build:clean": "rimraf dist",
    "build:server": "tsc -p src/tsconfig.json",
    "build:server:watch": "onchange -d 500 -k \"src/**/*.ts\" -- npm run build:server",
    "build:website": "webpack",
    "dev": "npm run build && concurrently \"npm run build:server:watch\" \"npm run start:watch\"",
    "doctoc": "doctoc --github --notitle README.md",
    "start": "node ./bin/www",
    "start:watch": "cross-env DEBUG=rps:* nodemon"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "debug": "~4.3.1",
    "express": "~4.17.1",
    "morgan": "~1.10.0",
    "pug": "3.0.0",
    "stylus": "0.54.8",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@types/stylus": "^0.48.33",
    "@types/webpack": "^4.41.25",
    "@types/ws": "^7.4.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "doctoc": "^1.4.0",
    "nodemon": "^2.0.6",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2",
    "vue": "^3.0.4",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "engines": {
    "node": ">= 14.0.0"
  }
}
